<!DOCTYPE html>
<html ng-app="InvestWatchApp" lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>InvestWatch</title>

  <!-- FONTS -->

  <!-- STYLESHEETS -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css">
  <link rel="stylesheet" href="/css/styles.css">
  <!-- EXT SCRIPTS -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.8/angular.min.js" charset="utf-8"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.8/angular-route.min.js" charset="utf-8"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.8/angular-cookies.min.js" charset="utf-8"></script>
  <!-- LOCAL SCRIPTS -->
  <script src="/js/main.js" charset="utf-8"></script>
  <script src="/js/controllers/search.js" charset="utf-8"></script>
  <script src="/js/controllers/list.js" charset="utf-8"></script>
  <script src="/js/controllers/detail.js" charset="utf-8"></script>
  <script src="/js/services/financeApi.js" charset="utf-8"></script>
  <script src="/js/services/usersApi.js" charset="utf-8"></script>
</head>
<body>
  <header ng-controller="SearchController">
    <div class="row">
      <div class="col s12 m6 l6">
        <img src="http://www.presentationpro.com/images/product/medium/slide/PPP_CSYMB_LT3_arrow_chart_up.jpg" alt="stock chart" id="logo">
        <a ng-href="#/"><h2 id="title">InvestWatch</h2></a>
      </div>
      <div class="col s12 m6 l6">
        <form id="header-form" ng-submit="searchTicker()">
          <label id='header-label'>Enter Ticker Symbol</label>
          <input id='header-search' type="text" ng-model="searchTerm" placeholder="AAPL">
          <button class='btn header-button' type="submit">Search</button>
        </form>
      </div>
    </div>
  </header>
  <div class="container">
    <div id="main-content" ng-view>

    </div>
  </div>
</body>
</html>
